{
  "name": "ning-ui-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "webpack-dev-server --config config/webpack.dev.js",
    "dev-admin": "cd admin && yarn dev",
    "dev-server": "export NODE_ENV='development' && node app.js",
    "dev-all": "yarn dev && yarn dev-admin && yarn dev-server",
    "build": "rimraf dist/index.html && rimraf dist/blog &&  webpack --config config/webpack.pro.js --progress --colors --profile",
    "build-admin": "cd admin && yarn dev",
    "pro-server": "export NODE_ENV='production' && node app.js",
    "test": "echo 'skip test...'",
    "inspect": "jsinspect ./src",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src"
  },
  "author": "zongyuan.ning",
  "license": "ISC",
  "pre-commit": [
    "eslint"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/*": "eslint --ext .js,.ts,.tsx",
    "**/*.{less,css}": "stylelint --fix"
  },
  "dependencies": {
